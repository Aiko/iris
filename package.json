{
  "name": "aiko-mail",
  "productName": "Aiko Mail",
  "version": "3.5.0",
  "description": "Aiko Mail - Email powered by AI",
  "main": "dist/app.js",
  "scripts": {
    "start": "electron-forge start",
    "restart": "clear; rm -rf dist; tsc; electron-forge start",
    "package": "electron-forge package",
    "make:forge": "electron-forge make",
    "publish": "electron-forge publish",
    "start:windows": "%APPDATA%\\npm\\electron-forge start",
    "restart:windows": "clear; rm -rf dist; tsc; %APPDATA%\\npm\\electron-forge start",
    "package:windows": "%APPDATA%\\npm\\electron-forge package",
    "make:windows": "%APPDATA%\\npm\\electron-forge make",
    "publish:windows": "%APPDATA%\\npm\\electron-forge publish",
    "enable-github": "export GITHUB_TOKEN=c4da6d8b5243d14d57e29c4afb2f91083cc6f0a1",
    "enable-github:windows": "set GITHUB_TOKEN=c4da6d8b5243d14d57e29c4afb2f91083cc6f0a1",
    "compile": "tsc",
    "clean": "rm -rf dist",
    "dry": "tsc; rm -rf dist",
    "make": "rm -rf dist; tsc",
    "make:icons": "python generate-intermediates.py",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [
    "email",
    "ai",
    "aiko"
  ],
  "author": {
    "name": "Mr. Seal ðŸ¦­",
    "email": "say@helloaiko.com"
  },
  "license": "Proprietary, all rights reserved, no permissions granted.",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "./public/assets/img/icon.icns",
        "asar": true
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "Aiko.Mail",
            "setupIcon": "./public/assets/img/icon.ico",
            "loadingGif": "./public/assets/img/loading.gif",
            "noDelta": false,
            "remoteReleases": "https://knidos.helloaiko.com"
          }
        },
        {
          "name": "@electron-forge/maker-dmg",
          "config": {
            "format": "ULFO",
            "icon": "./public/assets/img/icon.icns",
            "name": "Aiko Mail"
          }
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-auto-unpack-natives"
        ]
      ]
    }
  },
  "dependencies": {
    "@cliqz/adblocker-electron": "^1.20.2",
    "@sentry/electron": "^2.0.4",
    "@sentry/node": "^5.11.0",
    "aikomail-sdk": "github:Aiko/mail-sdk",
    "auto-bind": "^4.0.0",
    "cheerio": "^1.0.0-rc.3",
    "colors": "^1.4.0",
    "compromise": "^13.11.4",
    "cross-fetch": "^3.0.6",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^4.3.5",
    "emailjs-imap-client": "github:cuuupid/emailjs-imap-client",
    "express": "^4.17.1",
    "fs-extra": "^9.0.1",
    "google-auth-library": "^5.5.1",
    "googleapis": "^44.0.0",
    "html-to-text": "^6.0.0",
    "jdenticon": "^3.1.0",
    "jquery": "^3.4.1",
    "jsdom": "^16.4.0",
    "jsonwebtoken": "^8.5.1",
    "leveldown": "^5.6.0",
    "linvodb3": "^3.26.0",
    "mailparser": "github:cuuupid/mailparser",
    "nedb": "^1.8.0",
    "node-abi": "^2.19.3",
    "nodemailer": "^6.4.17",
    "nodemailer-juice": "^1.0.1",
    "nodemailer-plugin-inline-base64": "^2.1.1",
    "planer": "^1.1.1",
    "request": "^2.88.0",
    "ws": "^7.2.5"
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.50",
    "@electron-forge/maker-deb": "6.0.0-beta.50",
    "@electron-forge/maker-dmg": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "6.0.0-beta.50",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.50",
    "@electron-forge/maker-zip": "6.0.0-beta.50",
    "@electron-forge/plugin-auto-unpack-natives": "^6.0.0-beta.54",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.54",
    "@electron-forge/publisher-github": "^6.0.0-beta.54",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/nedb": "^1.8.12",
    "@types/nodemailer": "^6.4.4",
    "@types/request": "^2.48.7",
    "@types/ws": "^7.4.7",
    "electron": "^11.5.0"
  }
}
