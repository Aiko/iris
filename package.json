{
  "name": "aiko-mail",
  "productName": "Aiko Mail",
  "version": "3.0.5",
  "description": "Aiko Mail - Email powered by AI",
  "main": "app.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "start-windows": "%APPDATA%\\npm\\electron-forge start",
    "package-windows": "%APPDATA%\\npm\\electron-forge package",
    "make-windows": "%APPDATA%\\npm\\electron-forge make",
    "publish-windows": "%APPDATA%\\npm\\electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": {
    "name": "Priansh from Aiko ðŸ˜œ",
    "email": "priansh@helloaiko.com"
  },
  "license": "Proprietary, all rights reserved, no permissions granted.",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "./src/public/assets/img/icon.icns"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "aiko_mail",
            "setupIcon": "./src/public/assets/img/icon.ico",
            "loadingGif": "./src/public/assets/img/loading.gif"
          }
        },
        {
          "name": "@electron-forge/maker-dmg",
          "config": {
            "format": "ULFO",
            "icon": "./src/public/assets/img/icon.icns",
            "name": "Aiko Mail"
          }
        }
      ],
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "Aiko",
              "name": "aiko-mail-desktop"
            },
            "prerelease": false,
            "draft": false
          }
        }
      ]
    }
  },
  "dependencies": {
    "@sentry/electron": "^2.0.4",
    "@sentry/node": "^5.11.0",
    "aikomail-sdk": "github:Aiko/mail-sdk",
    "cheerio": "^1.0.0-rc.3",
    "colors": "^1.4.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^4.3.5",
    "emailjs-imap-client": "github:pshah123/emailjs-imap-client",
    "fs-extra": "^9.0.1",
    "google-auth-library": "^5.5.1",
    "googleapis": "^44.0.0",
    "html-to-text": "^6.0.0",
    "jquery": "^3.4.1",
    "jsonwebtoken": "^8.5.1",
    "leveldown": "^5.6.0",
    "linvodb3": "^3.26.0",
    "mailparser": "github:pshah123/mailparser",
    "node-abi": "^2.19.3",
    "node-email": "github:Aiko/node-email",
    "nodemailer": "^6.4.17",
    "nodemailer-juice": "^1.0.1",
    "nodemailer-plugin-inline-base64": "^2.1.1",
    "request": "^2.88.0",
    "ws": "^7.2.5"
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.50",
    "@electron-forge/maker-deb": "6.0.0-beta.50",
    "@electron-forge/maker-dmg": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "6.0.0-beta.50",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.50",
    "@electron-forge/maker-zip": "6.0.0-beta.50",
    "@electron-forge/publisher-github": "^6.0.0-beta.54",
    "electron": "^11.1.0"
  }
}
